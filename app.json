{
  "expo": {
    "name": "Med Tracker",
    "slug": "medi-tracking-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/newlogo.webp",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/newlogo.webp",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sksaakeeb0.meditrackingapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/newlogo.webp"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/newlogo.webp",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72ede83c-146b-4aab-a887-a68eca0e237a"
      }
    }
  }
}
